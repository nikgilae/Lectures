    "def fibonachi(n): \n",
    "  \n",
    "  if n== 0:\n",
    "    return 0\n",
    "  elif n == 1:\n",
    "    return 1\n",
    "  else:\n",
    "    return fibonachi(n - 1) + fibonachi(n - 2)\n",
    "\n",
    "print(fibonachi(n))"
